@model TASKORDER.Models.EditOrderViewModel



<h1>Edit Order</h1>
<br />
<form method="post" action="Edit">
    <div class="mb-3" hidden>
        <label for="" class="form-label"> Id</label>
        <input type="text" class="form-control" asp-for="Id" >
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Order Id</label>
        <input type="text" class="form-control" asp-for="OrderId" readonly>
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Order Name</label>
        <input type="text" class="form-control" asp-for="OrderName">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Order Date</label>
        <input type="date" class="form-control" asp-for="OrderDate">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Quantity</label>
        <input type="number" class="form-control" asp-for="Qty">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
    
</form>



